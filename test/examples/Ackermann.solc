contract Ackermann {

  data Nat = Zero | Succ[Nat]
  
  function test1 (x : Nat, y : Nat) {
    match x, y {
    | Zero, y1 => 1;
    | Succ[Zero], y2 => add(x1,y2);
    | Succ[Succ[x1]], y3 => 3 ;
    } ;
  }

  function test2 (x : Nat, y : Nat) {
    match x, y {
    | x1 , y1 => 1 ;
    | x2 , y2 => 2 ; 
    } ;
  }
}
