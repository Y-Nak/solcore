import Map ; 

contract ERC20 {
  _balances : Map[Address, Word]
  _allowances : Map[Address, Map [Address, Word]]
  _totalSupply : Word 

  _name : String 
  _symbol : String 

  constructor (name : String, symbol : String) {
    _name = name ;
    _symbol = symbol ; 
  }

  function name () -> String {
    _name; 
  }

  function symbol () -> String {
    _symbol ;  
  }

  function decimals () -> Word {
    18;
  }

  function totalSupply () -> Word {
    _totalSupply ;
  }

  function balanceOf (account : Address) -> Word {
     0;
  }
}
