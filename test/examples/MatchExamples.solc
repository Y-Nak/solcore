
contract MatchExamples {

  data Nat = Zero | Succ[Nat]
  
  function testCase4 (x : Nat, y : Nat) {
    match x, y {
    | Zero, y1 => 1;
    | Succ[Zero], y2 => 2;
    | Succ[Succ[x1]], y3 => 3 ;
    } ;
  }

  function testCase3 (x : Nat, y : Nat) {
    match x, y {
    | x1 , y1 => 1 ;
    | x2 , y2 => 2 ; 
    } ;
  }
  
  function testCase5(x : Nat, y : Nat) {
    match x, y {
    | Zero, Zero => 1 ;
    | x2, y2 => 2 ;
    | Succ[x1],Succ[y1] => 3 ;
    };
  }
}
